<div id="model-course-show-app"></div>

<script type="module">
  import { createApp } from "vue";
  import ModelCourseShow from "../components/ModelCourseShow.vue";

  document.addEventListener("DOMContentLoaded", () => {
    const target = document.getElementById("model-course-show-app");
    if (target) {
      const app = createApp(ModelCourseShow);
      app.mount(target);
    }
  });
</script>
