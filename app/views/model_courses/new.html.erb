<div id="model-course-new-app"></div>

<script type="module">
  import { createApp } from "vue";
  import ModelCourseNew from "../components/ModelCourseNew.vue";

  document.addEventListener("DOMContentLoaded", () => {
    const target = document.getElementById("model-course-new-app");
    if (target) {
      const app = createApp(ModelCourseNew);
      app.mount(target);
    }
  });
</script>
